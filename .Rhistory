colnames(dataco2)[1] = "country-pais"
colnames(dataco2)[2] = "ISO"
colnames(dataco2)[3] = "EmisionesCO2"
class(dataco2$EmisionesCO2)
dataco2$EmisionesCO2= as.numeric(dataco2$EmisionesCO2)
class(dataco2$EmisionesCO2)
data5= merge(data4,dataco2)
data5=data5[,-c(10)]
library(dplyr)
library(magrittr)
